
signature MVector
  ( MVector
  , read
  , write
  , swap
  , length
  ) where

import Prelude hiding (read, length, compare)

import Control.Monad.Primitive (PrimMonad(PrimState))

import Elem

data MVector s

read :: PrimMonad m => MVector (PrimState m) -> Int -> m Elem
write :: PrimMonad m => MVector (PrimState m) -> Int -> Elem -> m ()
swap :: PrimMonad m => MVector (PrimState m) -> Int -> Int -> m ()
length :: MVector s -> Int

